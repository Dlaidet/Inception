[server]

[mysqld]

[mysqld]
user = root
port = 3306
datadir = /var/lib/mysql
init-file = /db.sql
socket = /var/run/mysqld/mysqld.sock
skip-networking = false
bind-address = 0.0.0.0
skip-name-resolve

log_error = /var/log/mysql/error.log
expire_logs_days        = 10
character-set-server  = utf8mb4
collation-server      = utf8mb4_general_ci
